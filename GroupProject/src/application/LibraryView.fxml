            <Button fx:id="muteUnmute" layoutX="590.0" layoutY="434.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="35.0" text="M" />
            <Label layoutX="416.0" layoutY="368.0" prefHeight="21.0" prefWidth="44.0" text="Time:" />
            <Slider fx:id="seek" layoutX="415.0" layoutY="396.0" prefHeight="18.0" prefWidth="290.0" />
            <Button fx:id="uploadMusic" layoutX="13.0" layoutY="437.0" mnemonicParsing="false" onAction="#multiFileChooser" prefHeight="31.0" prefWidth="370.0" text="Add Media File" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
