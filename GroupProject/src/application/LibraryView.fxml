<?import javafx.scene.layout.AnchorPane?>

<AnchorPane maxHeight="480.0" maxWidth="720.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.LibraryController">
   <children>
      <AnchorPane layoutX="-1.0" layoutY="-1.0" prefHeight="480.0" prefWidth="720.0">
         <children>
            <TableView fx:id="libraryDisplay" layoutX="13.0" layoutY="13.0" prefHeight="420.0" prefWidth="370.0">
              <columns>
                <TableColumn prefWidth="285.0" text="Name" />
                <TableColumn fx:id="library" minWidth="0.0" prefWidth="84.0" text="Time" />
              </columns>
            </TableView>
            <ScrollBar fx:id="libraryScroller" layoutX="383.0" layoutY="13.0" orientation="VERTICAL" prefHeight="420.0" prefWidth="18.0" />
            <TextArea layoutX="415.0" layoutY="13.0" prefHeight="240.0" prefWidth="290.0" />
            <Button fx:id="play" layoutX="455.0" layoutY="434.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="35.0" text="▶" />
            <Button fx:id="backward" layoutX="415.0" layoutY="434.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="35.0" text="⏪" />
            <Button fx:id="pause" layoutX="495.0" layoutY="434.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="35.0" text="⏸" />
